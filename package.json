    "expo-camera": "~15.0.16",
    "expo-constants": "^16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-image-picker": "^15.0.5",
    "expo-linking": "^6.2.2",
    "expo-location": "~17.0.1",
    "expo-random": "~14.0.1",
    "expo-secure-store": "~13.0.2",
    "expo-sharing": "^12.0.1",
